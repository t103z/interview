{
 "swagger": "2.0",
 "info": {
  "version": "0.0.1",
  "title": "在线面试平台API文档",
  "description": "混合智能系统干预服务器API文档\n\n对于所有的PATCH请求，若payload中不包含某可选字段，则该字段不进行更新\n"
 },
 "host": "localhost:3010",
 "schemes": [
  "https"
 ],
 "produces": [
  "application/json"
 ],
 "basePath": "/api/v1",
 "paths": {
  "/aios_user": {
   "get": {
    "summary": "读取符合条件的 AIOS 用户列表",
    "description": "读取符合条件的 AIOS 用户列表\n",
    "tags": [
     "AIOS 用户",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "offset",
      "in": "query",
      "description": "从第几个用户开始读\n",
      "default": 0,
      "minimum": 0,
      "type": "integer",
      "format": "int32"
     },
     {
      "name": "limit",
      "in": "query",
      "description": "读取数量\n",
      "default": 10,
      "maximum": 100,
      "type": "integer",
      "format": "int32"
     },
     {
      "name": "sort",
      "in": "query",
      "description": "排序方式，取值为 last_active, id, name, 默认为 last_active ，表示按照最近活跃排序\n",
      "default": "recent",
      "type": "string"
     },
     {
      "name": "filter",
      "in": "query",
      "type": "string",
      "description": "筛选属性，取值为 status:serving, status:offline, 默认为空，表示无筛选条件\n"
     }
    ],
    "responses": {
     "200": {
      "description": "符合条件的 AIOS 用户列表",
      "schema": {
       "$ref": "#/definitions/AIOSUserList"
      }
     },
     "400": {
      "description": "读取错误",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/aios_user/{auid}": {
   "get": {
    "summary": "获取 AIOS 用户信息",
    "description": "获取 AIOS 用户信息\n",
    "tags": [
     "AIOS 用户",
     "withCredential"
    ],
    "parameters": [
     {
      "name": "auid",
      "in": "path",
      "description": "AIOS 用户 id\n",
      "required": true,
      "type": "string"
     }
    ],
    "produces": [
     "application/json"
    ],
    "responses": {
     "200": {
      "description": "AIOS 用户信息",
      "schema": {
       "$ref": "#/definitions/AIOSUser"
      }
     },
     "400": {
      "description": "AIOS 用户信息获取失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   },
   "patch": {
    "summary": "改变 AIOS 用户信息",
    "description": "改变 AIOS 用户信息\n",
    "tags": [
     "AIOS 用户",
     "withCredential"
    ],
    "parameters": [
     {
      "name": "body",
      "in": "body",
      "required": true,
      "schema": {
       "$ref": "#/definitions/_AIOSUser"
      }
     },
     {
      "name": "auid",
      "in": "path",
      "description": "AIOS 用户 id\n",
      "required": true,
      "type": "string"
     }
    ],
    "consumes": [
     "application/json"
    ],
    "produces": [
     "application/json"
    ],
    "responses": {
     "200": {
      "description": "状态改变成功",
      "schema": {
       "$ref": "#/definitions/AIOSUser"
      }
     },
     "400": {
      "description": "状态改变失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "403": {
      "description": "用户无权改变该 AIOS 用户信息",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/aios_user/{auid}/sessions": {
   "get": {
    "summary": "获取 AIOS 用户最近的会话列表",
    "description": "获取 AIOS 用户最近的会话列表\n",
    "tags": [
     "AIOS 用户",
     "会话",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "auid",
      "in": "path",
      "description": "AIOS 用户 id\n",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "AIOS 用户最近的会话列表",
      "schema": {
       "$ref": "#/definitions/SessionList"
      }
     },
     "400": {
      "description": "会话列表获取失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/session/{sid}": {
   "get": {
    "summary": "获取某一会话信息",
    "description": "获取某一会话信息\n",
    "tags": [
     "会话",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "sid",
      "in": "path",
      "description": "会话 id\n",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "某一会话信息",
      "schema": {
       "$ref": "#/definitions/Session"
      }
     },
     "400": {
      "description": "会话信息获取失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/session/{sid}/messages": {
   "get": {
    "summary": "获取某一会话对应的消息列表",
    "description": "获取某一会话对应的消息列表，消息按照轮数升序排列\n",
    "tags": [
     "消息",
     "会话",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "sid",
      "in": "path",
      "description": "会话 id\n",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "某一会话对应的消息列表",
      "schema": {
       "$ref": "#/definitions/MessageList"
      }
     },
     "400": {
      "description": "消息列表获取失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/session/{sid}/message/{turn}": {
   "get": {
    "summary": "获取某一会话对应的某一条消息",
    "description": "获取某一会话中的某一条消息\n",
    "tags": [
     "消息",
     "会话",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "sid",
      "in": "path",
      "description": "会话 id\n",
      "required": true,
      "type": "string"
     },
     {
      "name": "turn",
      "in": "path",
      "description": "对话轮数\n",
      "required": true,
      "type": "string"
     }
    ],
    "responses": {
     "200": {
      "description": "某一会话中的某一条消息",
      "schema": {
       "$ref": "#/definitions/Message"
      }
     },
     "400": {
      "description": "消息获取失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/session/{sid}/reply": {
   "post": {
    "summary": "向某一会话回复消息",
    "description": "向某一会话回复消息\n",
    "tags": [
     "消息",
     "会话",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "sid",
      "in": "path",
      "description": "会话 id\n",
      "required": true,
      "type": "string"
     },
     {
      "name": "body",
      "in": "body",
      "required": true,
      "schema": {
       "$ref": "#/definitions/_Message"
      }
     }
    ],
    "responses": {
     "200": {
      "description": "消息回复成功，返回轮数",
