       "$ref": "#/definitions/ReplyResult"
      }
     },
     "400": {
      "description": "消息回复失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "403": {
      "description": "用户无权回复给该会话",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  },
  "/session/{sid}/change_status": {
   "post": {
    "summary": "改变会话状态",
    "description": "干预师从界面操作来改变会话状态\n",
    "tags": [
     "会话",
     "状态",
     "withCredential"
    ],
    "produces": [
     "application/json"
    ],
    "parameters": [
     {
      "name": "sid",
      "in": "path",
      "description": "会话 id\n",
      "required": true,
      "type": "string"
     },
     {
      "name": "body",
      "in": "body",
      "required": true,
      "schema": {
       "$ref": "#/definitions/ChangeStatus"
      }
     }
    ],
    "responses": {
     "200": {
      "description": "状态改变结果",
      "schema": {
       "$ref": "#/definitions/ChangeStatusResult"
      }
     },
     "400": {
      "description": "状态改变失败",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "403": {
      "description": "用户无权改变该会话状态",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  }
 },
