{
 "swagger": "2.0",
 "info": {
  "description": "在线面试平台API文档",
  "version": "0.0.1",
  "title": "Interview Platform API Documents"
 },
 "host": "localhost:3030",
 "basePath": "/v1",
 "tags": [
  {
   "name": "user",
   "description": "平台用户相关API"
  },
  {
   "name": "candidate",
   "description": "候选人相关API"
  },
  {
   "name": "room",
   "description": "面试房间相关API"
  }
 ],
 "schemes": [
  "https"
 ],
 "paths": {
  "/room": {
   "get": {
    "tags": [
     "room"
    ],
    "summary": "获取所有面试房间",
    "produces": [
     "application/json"
    ],
    "responses": {
     "200": {
      "description": "符合条件的面试房间列表",
      "schema": {
       "$ref": "#/definitions/RoomList"
      }
     },
     "400": {
      "description": "读取错误",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     },
     "401": {
      "description": "用户未登录",
      "schema": {
       "$ref": "#/definitions/Error"
      }
     }
    }
   }
  }
 },
 "definitions": {
  "Status": {
   "type": "object",
   "required": [
    "status"
   ],
   "properties": {
    "status": {
     "type": "integer",
     "format": "int32"
    }
   }
  },
  "_Error": {
   "type": "object",
   "required": [
    "error"
   ],
   "properties": {
    "error": {
     "type": "string"
    }
   }
  },
  "Error": {
   "type": "object",
   "allOf": [
    {
     "$ref": "#/definitions/Status"
    },
    {
     "$ref": "#/definitions/_Error"
    }
   ],
   "example": {
    "status": "1",
    "error": "错误"
   }
  },
  "_Room": {
   "type": "object",
   "properties": {
    "id": {
     "type": "string",
     "description": "面试房间ID ，若不存在则置空"
    },
    "name": {
     "description": "面试房间名称",
     "type": "string"
    },
    "interviewerId": {
     "description": "面试官ID，若不存在则置空",
     "type": "string"
    },
    "interviewerName": {
     "description": "面试官名称，若不存在则置空",
     "type": "string"
    },
    "logo": {
     "description": "面试房间企业logo Url",
     "type": "string"
    }
   }
  },
  "Room": {
   "type": "object",
   "allOf": [
    {
     "$ref": "#/definitions/Status"
    },
    {
     "type": "object",
     "title": "Result",
     "required": [
      "result"
     ],
     "properties": {
      "result": {
       "$ref": "#/definitions/_Room"
      }
     }
    }
   ]
  },
  "_AIOSUserList": {
   "type": "object",
   "required": [
    "offset",
    "limit",
    "count",
    "users"
   ],
   "properties": {
    "offset": {
     "type": "integer",
     "format": "int32",
     "description": "面试房间列表的读取偏移"
    },
    "limit": {
     "type": "integer",
     "format": "int32",
     "description": "面试房间列表的读取个数，最大为100",
     "minimum": "0",
     "maximum": "100"
    },
    "count": {
     "description": "总面试房间数",
     "type": "integer",
     "format": "int32"
    },
    "users": {
     "description": "面试房间列表",
     "type": "array",
     "items": {
      "$ref": "#/definitions/_Room"
     }
    }
   }
  },
  "RoomList": {
   "type": "object",
   "allOf": [
    {
     "$ref": "#/definitions/Status"
    },
    {
     "type": "object",
     "title": "Result",
     "required": [
      "result"
     ],
     "properties": {
      "result": {
       "$ref": "#/definitions/_AIOSUserList"
      }
     }
    }
   ]
  }
 }
}